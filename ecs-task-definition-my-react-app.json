{
  "family": "my-react-app",
  "taskRoleArn": "arn:aws:iam::624615356298:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::624615356298:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "my-react-app",
      "image": "624615356298.dkr.ecr.eu-central-1.amazonaws.com/my-react-app:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "my-react-app",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "my-react-app"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  }
}
